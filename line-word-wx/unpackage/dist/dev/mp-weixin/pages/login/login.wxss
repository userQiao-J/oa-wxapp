.box-view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.logo {
  margin-top: 126rpx;
  width: 70%;
}
.logo-title {
  font-size: 48rpx;
  font-weight: bold;
  margin-top: 8%;
  color: #333;
}
.login-btn {
  width: 60%;
  position: absolute;
  bottom: 20%;
  left: 20%;
  box-sizing: border-box;
  background-color: #88a2f6;
  color: #FFF;
}
.login-btn:active {
  background-color: #6666ff;
}
.register-container {
  position: absolute;
  bottom: 50rpx;
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  color: #333;
}
.register-container .register {
  color: #88a2f6;
}

